{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    },

    "File": {
      "Discriminator": ""
    }
  },

  "AllowedHosts": "*",

  "Fido2": {
    "ServerDomain": "-TBD-",
    "ServerName": "-TBD-",
    "ServerIcon": "-TBD-",
    "Origins": [ "-TBD-" ],

    "ChallengeSize": 32
  },

  "PasskeyAuth": {
    "ActiveDirectory": {
      "Server": "-TBD-",
      "BaseOU": "-TBD-"
    },

    "ClaimSources": [
      {
        "ClaimType": "role",
        "PropertyName": "tokenGroupsGlobalAndUniversal",
        "OutputFormat": "sddl"
      },
      {
        "ClaimType": "samAccountName",
        "PropertyName": "samAccountName"
      },
      {
        "ClaimType": "userSid",
        "PropertyName": "objectSid",
        "OutputFormat": "sddl"
      },
      {
        "ClaimType": "sub",
        "PropertyName": "objectGuid",
        "OutputFormat": "guid"
      },
      {
        "ClaimType": "upn",
        "PropertyName": "userPrincipalName"
      }
    ],

    "_CSTemplate": {
      "ClaimType": "",
      "PropertyName": "",
      "ClaimValueDenyList": [ "", "" ]
    }
  },

  "DistributedCache": {
    "ConnectionString": "-TBD-",
    "SchemaName": "Passkeys",
    "TableName": "Session"
  },

  "SAML2": {
    "CertificatesPath": "-TBD-",
    "CertificatePassword": "-TBD-",

    "IDP": {
      "Issuer": "-TBD-",
      "SignatureAlgorithm": "http://www.w3.org/2001/04/xmldsig-more#rsa-sha256",
      "CertificateValidationMode": "ChainTrust", // "ChainTrust"
      "RevocationMode": "NoCheck"
    },

    "RelyingParties": [
      {
        "EntityId": "tbd",
        "MetadataUrl": "tbd"
      }
    ]
  }
